          <items id="description">Data element deprecated.</items>
          <items id="text">X - ptcO₂</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="comment">Measured via transcutaneous electrode oximetry, the partial pressure of carbon dioxide in the peripheral blood.</items>
          <items id="description">Data element deprecated.</items>
          <items id="text">X - ptcCO₂</items>
        </term_definitions>
